{
  "name": "@tinybigui/monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "Material Design 3 component library for React",
  "keywords": [
    "react",
    "material-design",
    "material-design-3",
    "ui-library",
    "tailwindcss",
    "components"
  ],
  "homepage": "https://github.com/buildinclicks/tinybigui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buildinclicks/tinybigui.git"
  },
  "license": "MIT",
  "author": "buildinclicks",
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "pnpm": {
    "overrides": {
      "react": "^18.3.1",
      "react-dom": "^18.3.1"
    }
  },
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "typecheck": "pnpm -r typecheck",
    "test": "pnpm -r test",
    "test:watch": "pnpm -r test:watch",
    "clean": "pnpm -r clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  }
}
