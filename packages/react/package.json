{
  "name": "@tinybigui/react",
  "version": "0.0.0",
  "description": "Material Design 3 React components built with Tailwind CSS",
  "keywords": [
    "react",
    "material-design",
    "material-design-3",
    "ui-library",
    "tailwindcss",
    "components",
    "md3"
  ],
  "homepage": "https://github.com/buildinclicks/tinybigui#readme",
  "bugs": {
    "url": "https://github.com/buildinclicks/tinybigui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buildinclicks/tinybigui.git",
    "directory": "packages/react"
  },
  "license": "MIT",
  "author": "buildinclicks",
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles.css": "./dist/styles.css",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "tailwindcss": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "tailwindcss": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public"
  }
}

